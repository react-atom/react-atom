{
  "name": "react-atom",
  "version": "independent",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "watch": "lerna run watch --stream",
    "build": "lerna run build",
    "publish-packages": "yarn build && lerna publish -m \"chore: publish packages\"",
    "examples": "yarn --cwd examples start"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  }
}
